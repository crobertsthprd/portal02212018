<cfoutput>
<!---
<span class="lgnusr">Logged in as <strong>#cookie.ufname# #cookie.ulname#</strong><br>
(#cookie.ulogin#)
<br>
Status: <strong>#cookie.ds#</strong><br><br></span>
--->
<br>

<cfif IsDefined("cookie.uID")>

	<cfif UseNewCodeMethod is 1>
		<!--- <cfset dopsds = request.reg_dsn> --->
		<!--- update session activity and fetch new registrations --->	
		<cfquery datasource="#dopsds#" name="GetNewRegistrations">
			select   coalesce(count(*), 0) as newregistrations
			FROM     dops.reghistory
			WHERE    primarypatronid = #cookie.uID#
			AND      pending = true
			and      ismiscfee = false
		</cfquery>
	
		<!--- <span class="sideblack">#GetNewRegistrations.newregistrations# new class(es)<BR>in basket --->
	
		<!--- <cfif GetNewRegistrations.newregistrations gt 0>
	
			<cfquery datasource="#dopsds#" name="GetIdleTime">
				SELECT   date_part('minutes', now() - sessionpatrons.lastactivitydt) as idletime
				FROM     sessionpatrons sessionpatrons
				         INNER JOIN patrons patrons ON sessionpatrons.patronid=patrons.patronid 
				WHERE    sessionpatrons.patronid = #cookie.uID# 
				AND      sessionpatrons.relationtype = 1
			</cfquery>
	
			<BR>
			(#GetIdleTime.idletime# inactivity minutes)
		</cfif> --->
	
	<cfelse>
		<!---
		<cfif session.classlist eq "''">
			<cfset session.classlist = "">
		</cfif>
		--->
		
		<!--- <span class="sideblack">#listlen(session.uniqueIDclasslist)# class(es) in </span><cfif listlen(session.uniqueIDclasslist) gt 0><A HREF="javascript:void(window.open('classes/mylist.cfm','','width=650,height=200,statusbar=0,scrollbars=1,resizable=0'))" class="lgnbig"><strong>basket</strong></a><cfelse><span class="sideblack">basket</span></cfif> --->
	</cfif>

</cfif>

<BR>
